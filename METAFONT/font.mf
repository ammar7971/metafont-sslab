def serif_i(expr lx, ly, rx, ry) =
	z51l = (rx, ly - serifHeight);
	z51r = (rx, ry + serifHeight);
	z52l = (lx - 3*(serifWidth-3), ly - serifHeight);
	z52r = (lx - 3*(serifWidth-3), ry + serifHeight);
	fill z51l -- z52l -- z52r -- z51r -- cycle;
enddef;
def serif_ii(expr lx, ly, rx, ry) =
	z61l = (rx, ly - serifHeight);
	z61r = (rx, ry + serifHeight);
	z62l = (lx - 3*(serifWidth-3), ly - serifHeight);
	z62r = (lx - 3*(serifWidth-3), ry + serifHeight);
	fill z61l -- z62l -- z62r -- z61r -- cycle;
enddef;
def serif_iii(expr lx, ly, rx, ry, curveRate) =
	z71l = (rx, ry + 0.5 * curveRate * (rx - lx));
	z71r = (rx + 0.5 * curveRate * (rx - lx), ry + 0.5 * curveRate * (rx - lx));
	z72l = (rx - 16*curveRate, ry + 0.5 * 0.5 * curveRate * (rx - lx));
	z72r = (rx + 0.5 * curveRate * (rx - lx), ry + 0.5 * 0.5 * curveRate * (rx - lx));
	z73l = (rx - 0.5 * 0.5 * curveRate * (rx - lx), ry + 16*curveRate);
	z73r = (rx - 0.5 * 0.5 * curveRate * (rx - lx), ry - 0.5 * curveRate * (rx - lx));
	z74l = (rx - 0.5 * curveRate * (rx - lx), ry);
	z74r = (rx - 0.5 * curveRate * (rx - lx), ry - 0.5 * curveRate * (rx - lx));
	
	z75l = (lx + 0.5 * curveRate * (rx - lx), ly);
	z75r = (lx + 0.5 * curveRate * (rx - lx), ly - 0.5 * curveRate * (rx - lx));
	z76l = (lx + 0.5 * 0.5 * curveRate * (rx - lx), ly + 16*curveRate);
	z76r = (lx + 0.5 * 0.5 * curveRate * (rx - lx), ly - 0.5 * curveRate * (rx - lx));
	z77l = (lx + 16*curveRate, ly + 0.5 * 0.5 * curveRate * (rx - lx));
	z77r = (lx - 0.5 * curveRate * (rx - lx), ly + 0.5 * 0.5 * curveRate * (rx - lx));
	z78l = (lx, ly + 0.5 * curveRate * (rx - lx));
	z78r = (lx - 0.5 * curveRate * (rx - lx), ly + 0.5 * curveRate * (rx - lx));
	unfill z71l .. z72l .. z73l .. z74l -- z74r .. z73r .. z72r .. z71r -- cycle;
	unfill z75l .. z76l .. z77l .. z78l -- z78r .. z77r .. z76r .. z75r -- cycle;
enddef;
def serif_iv(expr lx, ly, rx, ry, curveRate) =
	z81l = (rx, ry + 0.5 * curveRate * (rx - lx));
	z81r = (rx + 0.5 * curveRate * (rx - lx), ry + 0.5 * curveRate * (rx - lx));
	z82l = (rx - 16*curveRate, ry + 0.5 * 0.5 * curveRate * (rx - lx));
	z82r = (rx + 0.5 * curveRate * (rx - lx), ry + 0.5 * 0.5 * curveRate * (rx - lx));
	z83l = (rx - 0.5 * 0.5 * curveRate * (rx - lx), ry + 16*curveRate);
	z83r = (rx - 0.5 * 0.5 * curveRate * (rx - lx), ry - 0.5 * curveRate * (rx - lx));
	z84l = (rx - 0.5 * curveRate * (rx - lx), ry);
	z84r = (rx - 0.5 * curveRate * (rx - lx), ry - 0.5 * curveRate * (rx - lx));
	
	z85l = (lx + 0.5 * curveRate * (rx - lx), ly);
	z85r = (lx + 0.5 * curveRate * (rx - lx), ly - 0.5 * curveRate * (rx - lx));
	z86l = (lx + 0.5 * 0.5 * curveRate * (rx - lx), ly + 16*curveRate);
	z86r = (lx + 0.5 * 0.5 * curveRate * (rx - lx), ly - 0.5 * curveRate * (rx - lx));
	z87l = (lx + 16*curveRate, ly + 0.5 * 0.5 * curveRate * (rx - lx));
	z87r = (lx - 0.5 * curveRate * (rx - lx), ly + 0.5 * 0.5 * curveRate * (rx - lx));
	z88l = (lx, ly + 0.5 * curveRate * (rx - lx));
	z88r = (lx - 0.5 * curveRate * (rx - lx), ly + 0.5 * curveRate * (rx - lx));
	unfill z81l .. z82l .. z83l .. z84l -- z84r .. z83r .. z82r .. z81r -- cycle;
	unfill z85l .. z86l .. z87l .. z88l -- z88r .. z87r .. z86r .. z85r -- cycle;
enddef;
def serif_v(expr lx, ly, rx, ry, curveRate) =
	z191l = (lx + curveRate * (rx - lx), ly);
	z191r = (lx + curveRate * (rx - lx), ly - curveRate * (rx - lx));
	z192l = (lx + 0.5 * curveRate * (rx - lx), ly + 16*curveRate);
	z192r = (lx + 0.5 * curveRate * (rx - lx), ly - curveRate * (rx - lx));
	z193l = (lx + 16*curveRate, ly + 0.5 * curveRate * (rx - lx));
	z193r = (lx - curveRate * (rx - lx), ly + 0.5 * curveRate * (rx - lx));
	z194l = (lx, ly + curveRate * (rx - lx));
	z194r = (lx - curveRate * (rx - lx), ly + curveRate * (rx - lx));
	unfill z191l .. z192l .. z193l .. z194l -- z194r .. z193r .. z192r .. z191r -- cycle;
enddef;

						
